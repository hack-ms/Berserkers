<h1>Obras</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @works.each do |work| %>
      <tr>
        <td><%= work.id %></td>
        <td><%= work.idn_empreendimento %></td>
        <td><%= link_to 'Visualizar obra', work_path(work) %></td>
        <td><%= link_to 'Acompanhar obra', work_favorites_path(work) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


